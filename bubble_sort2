#second way to run loops in reverse fashion from 5 to 0 with diffrence of -1
def sort(list):
    for i in range(len(list)-1,0,-1):
        for j in range(i):
            if list[j]>list[j+1]:
                list[j],list[j+1]=list[j+1],list[j]

list = [5, 3, 8, 6, 7, 2,11,56,10]
sort(list)
print(list)