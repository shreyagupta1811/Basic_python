n=int(input("enter size of series:"))
def fab(n):
    a=0
    b=1
    if n<0:
        print("Wrong number entered")
    elif n==1:
        print(a)
    else:
        print(a,end="")
        print(b,end="")
        for i in range(2,n):
            c=a+b
            a=b
            b=c
            print(c,end="")

fab(n)