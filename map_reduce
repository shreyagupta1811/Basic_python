from functools import *
#def dub(n):
#   return n*2'''
#def add_all(a,b):
#    return a+b
num=[1,2,3,4,5,6,7,8,9]
evens=list(filter(lambda n:n%2==0,num))
print(evens)
#double=list(map(dub,evens))
double=list(map(lambda n:n*2,evens))
print(double)
#sum=reduce(add_all,double)
sum=reduce(lambda a,b:a+b,double)
print(sum)