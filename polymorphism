 #polymorphism means many form ex-the bird having similar swin ,walf fly than ot is called duck
 #DUCK TYPING
class myOwnIde:
    def execute(self):
        print("data type checking")
        print("spelling check")
        print("compile")
        print("debug")

class pycham:
     def execute(self):
         print("compile")
         print("debug")

class laptop:
     def code(self,ide):
        ide.execute()

ide2=myOwnIde()
ide1=pycham()
lap1=laptop()
#lap1.code(ide1)
lap1.code(ide2)